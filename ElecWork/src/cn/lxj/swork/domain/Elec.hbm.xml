<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE hibernate-mapping PUBLIC "
    -//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.lxj.swork.domain.Elec"  table="elec_test">
		<id name="eid" type="string">
			<column name="eid" sql-type="varchar(50)" not-null="true"/>
			<generator class="uuid"/>
		</id>
		
		<property name="ename" type="string">
			<column name="ename" sql-type="varchar(50)"/>
		</property>
		
		<property name="edate" type="date">
			<column name="edate" length="50"/>
		</property>
		<property name="eremark" type="string">
			<column name="eremark" sql-type="varchar(500)"/>
		</property>
	</class>

</hibernate-mapping>