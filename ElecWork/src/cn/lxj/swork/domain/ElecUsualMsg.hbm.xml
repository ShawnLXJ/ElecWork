<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE hibernate-mapping PUBLIC "
    -//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.lxj.swork.domain.ElecUsualMsg"  table="elec_usualmsg">
        <id name="uid" type="string">
            <column name="uid" sql-type="varchar(50)" not-null="true"/>
            <generator class="uuid"/>
        </id>
        
        <property name="machRun" type="string">
            <column name="machRun" sql-type="varchar(50)"/>
        </property>
        
        <property name="createTime" type="date">
            <column name="createTime" length="50"/>
        </property>
        <property name="staRun" type="string">
            <column name="staRun" sql-type="varchar(500)"/>
        </property>
    </class>

</hibernate-mapping>