<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE hibernate-mapping PUBLIC "
    -//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.lxj.swork.domain.ElecRolePopedom"  table="elec_role_popedom">
		<id name="roleId" type="string">
			<column name="roleId"  not-null="true"/>
		</id>
		
		<property name="popedomCode" type="string">
			<column name="popedomCode" sql-type="varchar(50)"/>
		</property>
		
		
		<property name="remark" type="string">
			<column name="remark" sql-type="varchar(500)"/>
		</property>
	</class>

</hibernate-mapping>